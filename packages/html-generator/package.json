{
  "name": "@vnhtml/html-generator",
  "description": "A module which generates self-contained HTML files from vnhtml scripts.",
  "version": "0.0.15",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/vnhtml.git"
  },
  "bin": "index.min.js",
  "files": [
    "index.min.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "compile": "npm-run-all compileBabel compileUglifyJs",
    "compileBabel": "babel index.js --out-file index.min.js",
    "compileUglifyJs": "uglifyjs index.min.js --compress --mangle --toplevel --output index.min.js",
    "watch": "npm-run-all watchBabel",
    "watchBabel": "babel index.js --watch --out-file index.min.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/node": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "jasmine": "^3.2.0",
    "npm-run-all": "^4.1.3",
    "rewire": "^4.0.1",
    "uglify-js": "^3.4.7"
  }
}
